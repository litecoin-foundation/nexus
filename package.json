    "react-native-svg": "^12.1.1",
    "react-native-touch-id": "^4.4.1",
    "react-native-webview": "7.6.0",
    "react-redux": "^7.2.4",
    "react-spring": "^9.2.4",
    "readable-stream": "^3.6.0",
