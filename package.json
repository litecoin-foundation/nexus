{
  "name": "lndmobile",
  "version": "0.0.1",
  "description": "Mobile Bitcoin & Litecoin lightning wallet built with React-Native + Redux",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/losh11/lndmobile.git"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "postinstall": "npm run protobuf && npx jetify",
    "protobuf": "pbjs src/lib/lightning/rpc.proto -t static-module -w es6 -o src/lib/lightning/rpc.js && pbjs src/lib/lightning/router.proto -t static-module -w es6 -o src/lib/lightning/router.js",
    "lint": "eslint .",
    "storybook": "start-storybook -p 7007",
    "prestorybook": "rnstl"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-native": "0.63.2",
    "@react-native-community/clipboard": "^1.2.2",
    "@react-native-community/masked-view": "^0.1.10",
