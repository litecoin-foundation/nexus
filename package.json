{
  "name": "lndmobile",
  "version": "0.0.1",
  "description": "Mobile Bitcoin & Litecoin lightning wallet built with React-Native + Redux",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/losh11/lndmobile.git"
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "postinstall": "yarn run protobuf && yarn run nodeify",
    "protobuf": "pbjs src/lib/lightning/rpc.proto -t static-module -w es6 -o src/lib/lightning/rpc.js",
    "nodeify": "rn-nodeify --install buffer,events,process,stream,util,inherits,fs,path --hack"
  },
  "dependencies": {
    "@react-native-community/slider": "^1.1.2",
    "axios": "^0.18.0",
    "base-x": "^3.0.5",
    "base64-js": "^1.3.0",
    "bech32": "^1.1.3",
    "bip21": "^2.0.2",
    "buffer": "^4.9.1",
    "events": "^1.1.1",
    "hash.js": "^1.1.7",
    "path-browserify": "0.0.0",
    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "protobufjs": "^6.8.8",
    "react": "16.8.3",
    "react-native": "0.59.4",
    "react-native-camera": "^2.9.0",
    "react-native-fs": "^2.13.3",
    "react-native-gesture-handler": "^1.2.1",
    "react-native-level-fs": "^3.0.1",
    "react-native-linear-gradient": "^2.5.4",
    "react-native-modal": "^11.0.1",
    "react-native-qrcode-svg": "^5.1.2",
    "react-native-sensitive-info": "^5.4.1",
    "react-native-svg": "^9.4.0",
    "react-navigation": "^3.10.1",
    "react-redux": "^7.1.0-rc.1",
    "readable-stream": "^3.3.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^5.10.0",
    "redux-persist-sensitive-storage": "^1.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rn-nodeify": "^10.0.1",
    "satoshi-bitcoin": "^1.0.4",
    "stream-browserify": "^2.0.2",
    "util": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "metro-react-native-babel-preset": "^0.53.1",
    "prettier": "^1.17.1",
    "react-test-renderer": "16.8.3"
  },
  "react-native": {
    "path": "path-browserify",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  }
}
